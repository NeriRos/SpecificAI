datasource db {
  provider = "postgresql"
  url      = "postgres://user:password@localhost:5432/db"
}

generator client {
  provider             = "prisma-client-py"
  interface            = "sync"
  recursive_type_depth = 5
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  password String
  name     String?

  @@index([email])
}
